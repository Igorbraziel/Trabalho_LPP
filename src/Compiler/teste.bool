class classeTeste
    vars y, x, w

    method subtracao()
    vars x
    begin
        y = 10
        x = 5
        w = x - y
        return w
    end-method

    method multiplicacao()
    vars x
    begin
        y = 10
        x = 5
        w = x * y
        return w
    end-method
end-class

main()
vars a, b, c, d, e
begin
    a = 60
    b = 50
    c = b - a
    b = new classeTeste
    a = b.y
    b.x = 20
    b.w = c
    d = b.w
    e = b.x
end
